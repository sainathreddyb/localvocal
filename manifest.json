{
  "manifest_version": 2,

  "name": "Traversy Launcher",
  "description": "LOCAL VOCAL",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js":["popup.js"]
    }
  ],
  "web_accessible_resources": [
    "data/data.json"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
          "storage"
        ]

}

