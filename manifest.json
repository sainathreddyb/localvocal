{
  "manifest_version": 2,

  "name": "LOCAL VOCAL",
  "description": "LOCAL VOCAL",
  "version": "1.0.0",
  "icons": { "128": "icon1c.png" },
  "browser_action": {
    "default_icon": "icon1.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js":["popup.js"]
    }
  ],
  "web_accessible_resources": [
    "data/data.json"
  ],
  "permissions": [
          "storage"
        ]

}

